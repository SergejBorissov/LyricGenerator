package de.borissov;

import java.util.ArrayList;
import java.util.List;

public class Präpostion {

   List<String> präWort = new ArrayList<>();

    public Präpostion(){
        präWort.add("über");
        präWort.add("zum");
        präWort.add("unter");
        präWort.add("auf");
        präWort.add("hinter");
    }

}
